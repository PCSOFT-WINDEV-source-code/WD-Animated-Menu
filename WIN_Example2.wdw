      // Summary: Opens the requested option
      // Syntax:
      //OpenMenu (<nOption>)
      //
      // Parameters:
      //	nOption: Identifier of option on which the click was performed
      // Return value:
      // 	None
      //
      
      procedure OpenMenu(nOption)
      
      
      // According to the option clicked
      switch nOption
      	case cstMENU_ASSISTANCE : Info("Opening the window for helping the users")
      	case cstMENU_CUSTOMERS : Info("Opening the window for managing the customers")
      	case cstMENU_ORDERS : Info("Opening the window for managing the orders")
      	case cstMENU_CONTRACTS : Info("Opening the window for managing the contracts")
      	case cstMENU_INVOICE : Info("Opening the window for managing the invoices")
      	case cstMENU_PRODUCTS : Open(WIN_Products)
      	case cstMENU_HUMANRESOURCES : Info("Opening the window for managing the contributors")
      	case cstMENU_STATS : Info("Opening the window for calculating the sale statistics")
      	case cstMENU_STOCK : Info("Opening the window for managing the stock")
      	other case : Error("This option is unknown.")
      end
