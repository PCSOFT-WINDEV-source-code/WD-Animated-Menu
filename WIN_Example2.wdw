         procedure SelectOption(local nIndex is int)
         
         
         // If the subscript is correct 
         if 0 < nIndex <= garrOptions.Count then
         	
         	// Defines this subscript as the current option
         	gnSelectedOption = nIndex
         	
         	// Redraws the menu
         	DrawMenu()
         	
         end
