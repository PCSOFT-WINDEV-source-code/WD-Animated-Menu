      // Summary: Preparing the animation 1
      // Syntax:
      //AnimationPrepare_1 ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      
      procedure AnimationPrepare_1()
      
      // Positions the buttons above the window
      BTN_PRODUCTS.Y = -BTN_PRODUCTS.Height
      BTN_CUSTOMERS.Y = -BTN_CUSTOMERS.Height
      BTN_COMMANDS.Y = -BTN_COMMANDS.Height
      
      // Hides the background image
      IMG_Interface.Visible = False
     type : 458752
   -
     name : AnimationPlay_1
     procedure_id : 1288460493466436335
     type_code : 14
     group : 3
     code : |1+
      // Summary: Reading the animation 1
      // Syntax:
      //AnimationPlay_1 ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      
      procedure AnimationPlay_1()
      
      // Animates the modification of the vertical position of the control
      AnimationPlayOnControlProperty(BTN_PRODUCTS, "Y", BTN_PRODUCTS.Y, 240, 50)
      AnimationPlayOnControlProperty(BTN_CUSTOMERS, "Y", BTN_CUSTOMERS.Y, 240, 50)
      AnimationPlayOnControlProperty(BTN_COMMANDS, "Y", BTN_COMMANDS.Y, 240, 50)
      
      // Prepares the animation
      AnimationPrepare(MyWindow,IMG_Interface.X, IMG_Interface.Y, IMG_Interface.Width, IMG_Interface.Height)
      // Displays the background image
      IMG_Interface.Visible = True
      AnimationPlay(animFadeIn, 100)
